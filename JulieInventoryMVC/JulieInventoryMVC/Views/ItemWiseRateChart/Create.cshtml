@using JulieInventoryMVC.App_Resources;
@{
    ViewBag.Title = "Create";
}

<link rel="stylesheet" type="text/css" href="~/Content/files/assets/icon/icofont/css/icofont.css">
<link rel="stylesheet" href="~/Content/files/bower_components/select2/css/select2.min.css" />
<link rel="stylesheet" type="text/css" href="~/Content/files/bower_components/bootstrap-multiselect/css/bootstrap-multiselect.css" />
<link rel="stylesheet" type="text/css" href="~/Content/files/bower_components/multiselect/css/multi-select.css" />
<style>

    input::-webkit-outer-spin-button,
    input::-webkit-inner-spin-button {
        -webkit-appearance: none;
        margin: 0;
    }
    /* Firefox */
    input[type=number] {
        -moz-appearance: textfield;
    }

    .form-control {
        border-left: none !important;
        border-top: none !important;
        border-right: none !important;
    }
</style>
<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">
                <div class="page-body">
                    <div class="row">
                        <div class="col-sm-12" style="float:left;">
                            <div class="card">
                                <div class="card-header">
                                    <div class="d-inline">
                                        <h5>Rate Chart Create</h5>
                                    </div>
                                </div>
                                    <div class="card-block">
                                        <form>
                                            <div class="row">

                                                <div class="col-sm-12 col-xl-4 m-b-30">
                                                    <div class="form-group form-primary">
                                                        <p>
                                                            Worker Section
                                                        </p>
                                                        <select class="js-example-basic-hide-search col-sm-12" multiple="multiple" style="width: 75%">
                                                            <optgroup label="Developer">
                                                                <option value="AL">Smith</option>
                                                                <option value="WY">Peter</option>
                                                                <option value="WY">James</option>
                                                                <option value="WY">Hanry Die</option>
                                                                <option value="WY">John Doe</option>
                                                                <option value="WY">Harry Poter</option>
                                                            </optgroup>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-12 col-xl-4 m-b-30">
                                                    <div class="form-group form-primary">
                                                        <p>
                                                            Item Section
                                                        </p>
                                                        <select class="js-example-basic-hide-search col-sm-12" multiple="multiple" style="width: 75%">
                                                            <optgroup label="Developer">
                                                                <option value="AL">Smith</option>
                                                                <option value="WY">Peter</option>
                                                                <option value="WY">James</option>
                                                                <option value="WY">Hanry Die</option>
                                                                <option value="WY">John Doe</option>
                                                                <option value="WY">Harry Poter</option>
                                                            </optgroup>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-12 col-xl-4 m-b-30">
                                                    <div class="form-group form-primary">
                                                        <p>
                                                            Work Name
                                                        </p>
                                                        <select class="js-example-basic-hide-search col-sm-12" multiple="multiple" style="width: 75%">
                                                            <optgroup label="Developer">
                                                                <option value="AL">Smith</option>
                                                                <option value="WY">Peter</option>
                                                                <option value="WY">James</option>
                                                                <option value="WY">Hanry Die</option>
                                                                <option value="WY">John Doe</option>
                                                                <option value="WY">Harry Poter</option>
                                                            </optgroup>
                                                        </select>
                                                    </div>

                                                </div>
                                                <div class="card-block" style="float:right">
                                                    <button class="btn waves-effect waves-light btn-success btn-outline-success" id="SaveBtn">
                                                        <i class="icofont icofont-check-circled"></i>Save
                                                    </button>
                                                    <button class="btn waves-effect waves-light btn-danger btn-outline-danger">
                                                        <i class="icofont icofont-eye-alt"></i>Cancel
                                                    </button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts{


    <script type="text/javascript" src="~/Content/files/bower_components/jquery-slimscroll/js/jquery.slimscroll.js"></script>
    <script type="text/javascript" src="~/Content/files/bower_components/modernizr/js/modernizr.js"></script>
    <script type="text/javascript" src="~/Content/files/bower_components/modernizr/js/css-scrollbars.js"></script>
    <script type="text/javascript" src="~/Content/files/bower_components/select2/js/select2.full.min.js"></script>
    <script type="text/javascript" src="~/Content/files/bower_components/bootstrap-multiselect/js/bootstrap-multiselect.js"></script>
    <script type="text/javascript" src="~/Content/files/bower_components/multiselect/js/jquery.multi-select.js"></script>
    <script type="text/javascript" src="~/Content/files/assets/js/jquery.quicksearch.js"></script>
    <script type="text/javascript" src="~/Content/files/assets/pages/advance-elements/select2-custom.js"></script>
    <script src="~/Content/files/assets/js/pcoded.min.js"></script>
    <script src="~/Content/files/assets/js/vertical/vertical-layout.min.js"></script>
    <script src="~/Content/files/assets/js/jquery.mCustomScrollbar.concat.min.js"></script>
}